{
  "scripts": {
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "lint": "next lint",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",

    "build:worker": "wrangler pages build",
    "dev:worker": "wrangler dev --port 3001",
    "preview:worker": "npm run build && npm run dev:worker",
    "deploy:worker": "wrangler pages deploy"
  },
  "dependencies": {
    "cloudflare": "^3.5.0",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.7",
    "@opennextjs/cloudflare": "^0.3.3",
    "@types/react": "19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-next-on-pages": "^1.13.7",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "wrangler": "^3.99.0"
  }
}
